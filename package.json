{
  "version": "0.0.0",
  "name": "solid-fluent-store",
  "description": "Solid Fluent Store is a wrapper library for Solid stores. It provides a fluent and type-safe API to interact with a store while maintaining read/write segregation.",
  "license": "MIT",
  "author": "Jorren",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jorren/solid-fluent-store.git"
  },
  "homepage": "https://github.com/Jorren/solid-fluent-store#readme",
  "bugs": {
    "url": "https://github.com/Jorren/solid-fluent-store/issues"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "exports": {
    "solid": {
      "import": "./dist/index.jsx"
    },
    "browser": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "test": "jest && pnpm typecheck",
    "build": "tsup",
    "prepublishOnly": "pnpm build",
    "format": "prettier --ignore-path .gitignore -w \"src/**/*.{js,ts,json,css,tsx,jsx}\"",
    "update-deps": "pnpm up -Li",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "solid-js": "^1.6.9"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@jest/globals": "^29.3.1",
    "babel-jest": "^29.3.1",
    "esbuild": "^0.17.0",
    "esbuild-plugin-solid": "^0.4.2",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "prettier": "2.8.3",
    "solid-jest": "^0.2.0",
    "solid-js": "^1.6.9",
    "tsup": "^6.5.0",
    "tsup-preset-solid": "^0.0.5",
    "typescript": "^4.9.4"
  },
  "keywords": [
    "solid",
    "store",
    "fluent"
  ],
  "packageManager": "pnpm@7.20.0",
  "jest": {
    "preset": "solid-jest/preset/browser",
    "testEnvironment": "jsdom"
  }
}
